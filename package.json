{
  "name": "next-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:dev": "export NEXT_PUBLIC_DEPLOY_ENV=development && next build",
    "commitlint": "commitlint --edit",
    "dev": "next dev",
    "lint": "next lint",
    "pre-commit": "npx lint-staged",
    "pre-push": "tsc --noEmit",
    "prepare": "husky",
    "prettier": "prettier --write --cache .",
    "start": "next start"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js}": [
      "prettier --write",
      "next lint --fix --file"
    ],
    "**/*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "axios": "^1.9.0",
    "case": "^1.6.3",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "i18next": "^25.0.2",
    "lodash": "^4.17.21",
    "next": "15.3.1",
    "next-i18next": "^15.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint-react/eslint-plugin": "^1.52.2",
    "@eslint/eslintrc": "^3",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
